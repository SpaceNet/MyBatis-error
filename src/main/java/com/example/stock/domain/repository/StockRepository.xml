<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.stock.domain.repository.StockRepository">

    <select id="findOne" parameterType="integer" resultType="com.example.stock.domain.model.Stock">
        SELECT
            *
        FROM
            stock
        WHERE
            id = #{id}
    </select>

    <select id="findAllLimit" resultType="com.example.stock.domain.model.Stock">
        SELECT
            *
        FROM
            stock
        limit 
            2000
    </select>

</mapper>